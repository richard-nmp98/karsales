{
  "name": "kar-website",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1",
    "node": ">=10.13"
  },
  "scripts": {
    "start": "CHOKIDAR_USEPOLLING=true next dev",
    "build": "next build",
    "export": "next export",
    "build-export": "next build && next export",
    "analyze": "cross-env ANALYZE=true next build",
    "prod": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write --ignore-unknown '**/*.{js,jsx,ts,tsx,md,mdx,svg}'",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx --ext .gql --ext .graphql",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "schema:codegen": "gqlg --schemaFilePath ./schema/schema.graphql --destDirPath ./__generated__ --enableOriginalQuery --depthLimit 5 && prettier --write --loglevel silent __generated__/**/*.gql",
    "codegen": "graphql-codegen --config codegen/codegen.js && yarn schema:codegen",
    "typegen": "graphql-codegen --config codegen/codegen.yml"
  },
  "proxy": "http://api-dev.karsales.asia",
