    "lzutf8": "^0.6.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.33",
    "next": "13.1.6",
    "next-secure-headers": "^2.2.0",
    "next-seo": "^5.15.0",
    "next-urql": "^4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
